{
  "name": "moonhub",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "prisma:push:dev": "dotenv -e .env.dev prisma db push",
    "prisma:push:test": "dotenv -e .env.test prisma db push",
    "prisma:generate": "dotenv -e .env.dev prisma generate",
    "setup:dev": "dotenv -e .env.dev bash ./scripts/setup_dev.sh && bun run prisma:push:dev",
    "setup:test": "dotenv -e .env.test bash ./scripts/setup_integration.sh && bun run prisma:push:test",
    "test": "dotenv -e .env.test vitest run",
    "integration": "bun run setup:test && bun run test",
    "prebuild": "prisma generate",
    "build": "vite build",
    "start": "bun run server/server.ts",
    "dev": "dotenv -e .env.dev bun run start",
    "prod": "dotenv -e .env.prod bun run start"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/bun": "latest",
    "@types/eslint": "^8.56.2",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.55",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "install": "^0.13.0",
    "npm": "^10.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "vike": "^0.4.161",
    "vike-react": "^0.4.4",
    "vite": "^5.1.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@chatscope/chat-ui-kit-react": "^1.10.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.9",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.15.9",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.9.1",
    "@sinclair/typebox": "^0.32.14",
    "@sinclair/typebox-codegen": "^0.9.7",
    "@tanstack/react-query": "^5.20.1",
    "@trpc/client": "next",
    "@trpc/server": "next",
    "express": "^4.18.2",
    "fast-equals": "^5.0.1",
    "mutative": "^1.0.3",
    "openai": "^4.28.0",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "source-map": "^0.7.4",
    "source-map-support": "^0.5.21",
    "vike-react-query": "^0.0.3"
  }
}